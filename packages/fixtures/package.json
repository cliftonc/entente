{
  "name": "@entente/fixtures",
  "version": "0.1.12",
  "description": "Fixture management utilities for Entente contract testing",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run"
  },
  "dependencies": {
    "@entente/types": "workspace:*",
    "graphql": "^16.8.1",
    "js-yaml": "^4.1.0",
    "path-to-regexp": "^6.3.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "@types/node": "^22.0.0",
    "@types/js-yaml": "^4.0.8"
  },
  "keywords": [
    "contract-testing",
    "fixtures",
    "testing"
  ]
}
