  "dependencies": {
    "@entente/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "contract-testing",
    "provider",
    "verification",
    "testing"
  ]
}
