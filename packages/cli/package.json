{
  "name": "@entente/cli",
  "version": "0.1.0",
  "description": "Command line interface for Entente contract testing",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "entente": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "dependencies": {
    "@entente/fixtures": "workspace:*",
    "@entente/types": "workspace:*",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "open": "^10.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": ["contract-testing", "cli", "deployment", "fixtures"]
}
