{
  "name": "@entente/metadata",
  "version": "0.1.0",
  "description": "Multi-language package metadata extraction for Entente",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.2"
  },
  "keywords": [
    "metadata",
    "package",
    "multi-language",
    "node",
    "go",
    "rust",
    "python",
    "ruby"
  ]
}
