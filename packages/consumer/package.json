{
  "name": "@entente/consumer",
  "version": "0.1.16",
  "description": "Consumer testing library for Entente contract testing",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm && pnpm run build:cjs && pnpm run build:cjs-package",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs-package": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "dev": "tsc --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run"
  },
  "dependencies": {
    "@entente/fixtures": "workspace:*",
    "@entente/metadata": "workspace:*",
    "@entente/types": "workspace:*",
    "@mswjs/interceptors": "^0.39.6",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "@types/ws": "^8.5.10",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "contract-testing",
    "consumer",
    "testing",
    "mocking"
  ]
}
