{
  "name": "@entente/consumer",
  "version": "0.1.1",
  "description": "Consumer testing library for Entente contract testing",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run"
  },
  "dependencies": {
    "@entente/fixtures": "workspace:*",
    "@entente/types": "workspace:*"
  },
  "peerDependencies": {
    "@stoplight/prism-http": "^5.8.0"
  },
  "devDependencies": {
    "@stoplight/prism-http": "^5.8.0",
    "@stoplight/prism-core": "^5.8.0",
    "@types/node": "^22.10.6",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": ["contract-testing", "consumer", "testing", "mocking"]
}
