{
  "name": "@entente/consumer",
  "version": "0.1.14",
  "description": "Consumer testing library for Entente contract testing",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run"
  },
  "dependencies": {
    "@entente/fixtures": "workspace:*",
    "@entente/types": "workspace:*",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "@types/ws": "^8.5.10"
  },
  "keywords": [
    "contract-testing",
    "consumer",
    "testing",
    "mocking"
  ]
}
